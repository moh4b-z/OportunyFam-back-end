NODE_ENV=production
WEB_CONCURRENCY=4

# Cache TTLs (em segundos)
CACHE_TTL_INSTITUICOES=30
CACHE_TTL_ATIVIDADES=15
CACHE_TTL_MENSAGENS=5

# Redis (opcional, mas RECOMENDADO para 10k+ req/s)
# REDIS_URL=redis://127.0.0.1:6379

# Rate Limiting
SKIP_RATE_LIMIT=0
RATE_LIMIT_MAX=1000
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_WHITELIST=127.0.0.1,::1

# Server Timeouts
KEEP_ALIVE_TIMEOUT=65000
HEADERS_TIMEOUT=66000

# Logging (IMPORTANT: Desabilitar em prod para performance)
DISABLE_REQUEST_LOGGING=1
LOG_LEVEL=error
ENABLE_MEMORY_MONITOR=0

# Database
DATABASE_URL_MYSQL=mysql://user:pass@host:3306/db?sslaccept=strict

# Frontend (CORS)
FRONTEND_ORIGIN=*

# Port
PORT=8080
